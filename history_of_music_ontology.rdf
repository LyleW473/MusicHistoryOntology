        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEnsembleMembership"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
        <rdfs:comment>Specifies the time interval a member/individual has been inside of a music ensemble.</rdfs:comment>
        <rdfs:label>has time interval for a MusicEnsembleMembership.</rdfs:label>
    </ObjectProperty>
    


