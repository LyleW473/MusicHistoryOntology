        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Musician"/>
        <rdfs:comment xml:lang="en">Indicates the name of the person or people who wrote the lyrics of a song.</rdfs:comment>
        <rdfs:label xml:lang="en">has lyricist</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasMembershipInterval -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasMembershipInterval">
