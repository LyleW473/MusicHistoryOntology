                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-meta/CreativeProcess"/>
                </unionOf>
            </Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The relation between an entity and a situation in which such entity is member of a collection.</rdfs:comment>
        <rdfs:label xml:lang="en">Is involved in</rdfs:label>
    </ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/isLanguageOf -->

    <ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/isLanguageOf">
