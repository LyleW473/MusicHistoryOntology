    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/MusicGenre -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/music-meta/MusicGenre">
        <rdfs:subClassOf>
            <Restriction>
