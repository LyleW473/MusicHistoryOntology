    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dati.beniculturali.it/cis/CulturalEvent -->

    <Class rdf:about="http://dati.beniculturali.it/cis/CulturalEvent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/arco/ontology/core/Event"/>
        <rdfs:comment xml:lang="en">Represents a cultural event as defined within the Italian Cultural Heritage framework. This class includes events such as exhibitions, performances, and festivals that contribute to the cultural and artistic landscape.</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural Event</rdfs:label>
    </Class>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Album -->

    <Class rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Album">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Release"/>
        <rdfs:comment xml:lang="en">A collection of musical recordings released together as a single cohesive work. Albums typically consist of multiple tracks, created by one or more artists, and are presented under a unified theme, concept, or artistic vision. Albums are distributed in various formats, including digital streaming, CDs, vinyl records, and digital downloads.</rdfs:comment>
        <rdfs:label xml:lang="en">Album</rdfs:label>
    </Class>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Continent -->

    <Class rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Continent">
        <rdfs:comment xml:lang="en">A continent is defined as a large, continuous expanse of land, typically separated by oceans from other major landmasses. It serves as a primary geographical division of the Earth, encompassing multiple countries and distinct cultural, political, and ecological regions.</rdfs:comment>
        <rdfs:label xml:lang="en">Continent</rdfs:label>
    </Class>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Opera -->

    <Class rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Opera">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:comment xml:lang="en">A dramatic musical work that combines text (libretto) and musical score, often performed in a theatrical setting with singers and orchestra. It typically contains multiple acts and scenes.</rdfs:comment>
        <rdfs:label xml:lang="en">Opera</rdfs:label>
    </Class>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#PerformanceEvent -->

    <Class rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#PerformanceEvent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/arco/ontology/core/Event"/>
        <rdfs:comment xml:lang="en">An occasion or gathering where musical works are presented live to an audience. This includes concerts, recitals, operas, musicals, and other public or private performances involving vocal or instrumental music. Performance events may feature individual artists, ensembles, orchestras, or bands, and often occur in venues such as concert halls, theaters, festivals, or informal settings.</rdfs:comment>
        <rdfs:label xml:lang="en">Performance Event</rdfs:label>
    </Class>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#RecordLabel -->

    <Class rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#RecordLabel">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Organization"/>
        <rdfs:comment xml:lang="en">An organization responsible for managing the production, distribution, marketing, and promotion of musical recordings and artists. A record label typically signs artists, oversees the recording process, provides creative direction, and facilitates the commercial availability of music through various formats and platforms.</rdfs:comment>
        <rdfs:label xml:lang="en">Record Label</rdfs:label>
    </Class>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Single -->

    <Class rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Single">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Release"/>
        <rdfs:comment xml:lang="en">A musical recording released individually, typically highlighting one primary track intended for promotional purposes or as a standalone artistic release. Singles often serve to promote upcoming albums or showcase an artist’s new material and are distributed through digital platforms, radio, or physical formats.</rdfs:comment>
        <rdfs:label xml:lang="en">Single</rdfs:label>
    </Class>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Song -->

    <Class rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#Song">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:comment xml:lang="en">A musical composition typically meant for vocal performance, either as a standalone piece or part of a larger work. It may include lyrics and instrumental accompaniment.</rdfs:comment>
        <rdfs:label xml:lang="en">Song</rdfs:label>
    </Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <Class rdf:about="http://www.w3.org/2002/07/owl#Thing"/>
    


    <!-- https://w3id.org/MON/person.owl#Female -->

    <Class rdf:about="https://w3id.org/MON/person.owl#Female">
        <rdfs:subClassOf rdf:resource="https://w3id.org/MON/person.owl#Person"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Musician"/>
        <disjointWith rdf:resource="https://w3id.org/MON/person.owl#Male"/>
    </Class>
    


    <!-- https://w3id.org/MON/person.owl#Male -->

    <Class rdf:about="https://w3id.org/MON/person.owl#Male">
        <rdfs:subClassOf rdf:resource="https://w3id.org/MON/person.owl#Person"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Musician"/>
    </Class>
    


    <!-- https://w3id.org/MON/person.owl#Person -->

    <Class rdf:about="https://w3id.org/MON/person.owl#Person">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:comment xml:lang="en">A human being regarded as an individual.</rdfs:comment>
        <rdfs:label xml:lang="en">Person</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/arco/ontology/core/Event -->

    <Class rdf:about="https://w3id.org/arco/ontology/core/Event">
        <rdfs:comment xml:lang="it">Questa classe rappresenta qualsiasi processo, evento o stato mentale, fisico o sociale.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents any physical, social, or mental process, event, or state.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/core"/>
        <rdfs:label xml:lang="en">Event</rdfs:label>
        <rdfs:label xml:lang="it">Evento</rdfs:label>
        <versionInfo xml:lang="it">instabile</versionInfo>
        <versionInfo xml:lang="en">unstable</versionInfo>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Address -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Address">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Place"/>
        <rdfs:comment xml:lang="en">Defines a structured postal address, providing detailed location information (street, city, postal code, etc.) for venues, studios, or residences associated with musical activities.</rdfs:comment>
        <rdfs:label xml:lang="en">Address</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Agent -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Agent">
        <rdfs:comment xml:lang="en">Any agentive Object, either physical (e.g. a person, a robot), or social (e.g. a corporation, an institution, a community)</rdfs:comment>
        <rdfs:label xml:lang="en">Agent</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/AgentRole -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/AgentRole">
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/involvesRole"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/core/Role"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/involvesRole"/>
                <allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/core/Role"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The situation of an agent having a role.</rdfs:comment>
        <rdfs:label xml:lang="en">Agent role</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/City -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/City">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Place"/>
        <rdfs:comment xml:lang="en">Represents an urban area or municipality, commonly used to identify the location of musical events, cultural institutions, or the origin of artists within a geographic context.</rdfs:comment>
        <rdfs:label xml:lang="en">City</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Country -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Country">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Place"/>
        <rdfs:comment xml:lang="en">Denotes a sovereign state or nation, offering a broad geographic context that can categorize musical works, artist origins, or event locations at a national level.</rdfs:comment>
        <rdfs:label xml:lang="en">Country</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Description -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Description">
        <rdfs:comment rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/description.owl"/>
        <rdfs:comment xml:lang="en">A Description represents a conceptualization. It can be thought also as a &apos;descriptive context&apos; that defines concepts in order to see a &apos;relational context&apos; out of a set of data or observations. For example, a Plan is a description of some actions to be executed by agents in a certain way, with certain parameters; a Diagnosis is a description that provides an interpretation to a set of observed entities, etc.
A Description typically defines at least one Concept, for example the &apos;preparing a coffee&apos; Description could define concepts such as &apos;Heater&apos;, &apos;AmountOfWater&apos;, &apos;CoffeeMix&apos;, etc.
With respect to other patterns, descriptions are &apos;abstractions&apos; of situations (cf. the &apos;situation&apos; pattern), and in some complex use cases (e.g. matching executions to plan models), the two patterns can be composed (see the descriptionsituation pattern).</rdfs:comment>
        <rdfs:label xml:lang="en">Description</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/InformationObject -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/InformationObject">
        <rdfs:comment xml:lang="en">A piece of information, such as a musical composition, a text, a word, a picture, independently from how it is concretely and phisically realized.</rdfs:comment>
        <rdfs:label xml:lang="en">Information object</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/InformationRealization -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/InformationRealization">
        <rdfs:comment xml:lang="en">A concrete and physical realization of an information object.</rdfs:comment>
        <rdfs:label xml:lang="en">Information realization</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Language -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Language">
        <disjointWith rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Lyrics"/>
        <rdfs:comment xml:lang="en">Represents the natural language used in texts. This includes languages such as English, Spanish, French, etc., which are essential for understanding and interpreting musical works.</rdfs:comment>
        <rdfs:label xml:lang="en">Language</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/MusicTimeDuration -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/MusicTimeDuration">
        <rdfs:comment xml:lang="en">Denotes the duration of a musical performance, recording, or segment. It is typically measured in minutes, seconds, or beats, and reflects the overall length of the musical piece.</rdfs:comment>
        <rdfs:label xml:lang="en">Music Time Duration</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Organization -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Organization">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:comment xml:lang="en">An internally structured, conventionally created social agent, needing a specific role and agent that plays it, in order to act.</rdfs:comment>
        <rdfs:label xml:lang="en">Organization</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Place -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Place">
        <rdfs:comment xml:lang="en">Anything that is intended as a location for something else, e.g. geographical places, spatial objects.</rdfs:comment>
        <rdfs:label xml:lang="en">Place</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Role -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Role">
        <rdfs:comment xml:lang="en">A concept that classifies an object.</rdfs:comment>
        <rdfs:label xml:lang="en">Role</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Street -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Street">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/Place"/>
        <rdfs:comment xml:lang="en">Represents a roadway or street name, typically used as part of a complete address to pinpoint specific locations such as concert venues, recording studios, or artist residences.&quot;</rdfs:comment>
        <rdfs:label xml:lang="en">Street</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/TimeInterval -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/TimeInterval">
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/hasTime"/>
                <someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/hasEndTime"/>
