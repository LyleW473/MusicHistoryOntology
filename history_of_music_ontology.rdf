        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">This property relates a synonym to an entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/core"/>
        <rdfs:label xml:lang="en">has synonym</rdfs:label>
        <versionInfo xml:lang="it">instabile</versionInfo>
        <versionInfo xml:lang="en">unstable</versionInfo>
    </DatatypeProperty>
    


    <!-- https://w3id.org/arco/ontology/core/isCurrent -->

    <DatatypeProperty rdf:about="https://w3id.org/arco/ontology/core/isCurrent">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/core/Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">This property indicates if a situation or an event are current or not, when the time interval is not available</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/core"/>
        <rdfs:label xml:lang="en">is current</rdfs:label>
        <versionInfo xml:lang="it">instabile</versionInfo>
        <versionInfo xml:lang="en">unstable</versionInfo>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasActivityEndDate -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasActivityEndDate">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasEndTime"/>
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-meta/MusicArtist"/>
                </unionOf>
            </Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Denotes the end date of a musical activity or event, marking the conclusion of the event’s scheduled timeframe.</rdfs:comment>
        <rdfs:label xml:lang="en">has activity end date</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasActivityStartDate -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasActivityStartDate">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasStartTime"/>
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-meta/MusicArtist"/>
                </unionOf>
            </Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Denotes the start date of a musical activity or event, marking the commencement of the event’s scheduled timeframe.</rdfs:comment>
        <rdfs:label xml:lang="en">has activity start date</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasEndTime -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasEndTime">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasTime"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">The end time of a time interval.</rdfs:comment>
        <rdfs:label xml:lang="en">has end time</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasFullAddress -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasFullAddress">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/Address"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">Specifies the complete address details of a venue, recording studio, or other location associated with a musical event or artist&apos;s contact information.</rdfs:comment>
        <rdfs:label xml:lang="en">has full address</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasName -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasName">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">Represents the complete name of an individual in the music domain, serving as a general identifier.</rdfs:comment>
        <rdfs:label xml:lang="en">has name</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasNickname -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasNickname">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasName"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">A familiar name given to a person or thing instead of or as well as the real name.</rdfs:comment>
        <rdfs:label xml:lang="en">has nickname</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasStartTime -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasStartTime">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasTime"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">The start time of a time interval.</rdfs:comment>
        <rdfs:label xml:lang="en">has start time</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasText -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasText">
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Thing"/>
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-meta/DedicationStatement"/>
                </unionOf>
            </Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Holds the lyrical content or written annotations related to a musical work, including song lyrics, performance notes, or other text-based information.</rdfs:comment>
        <rdfs:label xml:lang="en">has text</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasTime -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasTime">
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Represents the temporal aspects of a musical work. This property can denote the duration of a track, the timing of a performance, or details of a recording session.</rdfs:comment>
        <rdfs:label xml:lang="en">has time</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasTitle -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasTitle">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Denotes the name of a musical work, such as a song or composition, providing a concise identifier for the piece.</rdfs:comment>
        <rdfs:label xml:lang="en">has title</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/hasOrderNumber -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/music-meta/hasOrderNumber">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/AbstractScore"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Represents the sequence or order number of a musical element, such as the track number within an album, establishing its position in a sequence.</rdfs:comment>
        <rdfs:label xml:lang="en">has order number</rdfs:label>
    </DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/hasTempo -->

    <DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/music-meta/hasTempo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/AbstractScore"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">Specifies the pace of a musical piece, typically measured in beats per minute (BPM). It indicates the energy and rhythmic drive of the composition.</rdfs:comment>
        <rdfs:label xml:lang="en">has tempo</rdfs:label>
    </DatatypeProperty>
    


    <!-- 
