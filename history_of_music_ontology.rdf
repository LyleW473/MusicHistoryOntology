        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/InformationRealization"/>
        <rdfs:comment xml:lang="en">A relation between an information object and its realization (an information realization).</rdfs:comment>
        <rdfs:label xml:lang="en">is realized by</rdfs:label>
    </ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/isTimeIntervalOf -->

    <ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/isTimeIntervalOf">
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">Indicates that a given time interval is associated with a musical event or performance, marking the period during which the event occurs.</rdfs:comment>
        <rdfs:label xml:lang="en">is time interval of</rdfs:label>
    </ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/isTypeOf -->

    <ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/isTypeOf">
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/Type"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">Specifies that a musical element is an instance of a particular type or category, serving as the inverse of hasType to support classification of musical entities.</rdfs:comment>
        <rdfs:label xml:lang="en">is type of</rdfs:label>
    </ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/realizes -->

    <ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/realizes">
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/InformationRealization"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/InformationObject"/>
        <rdfs:comment xml:lang="en">A relation between an information realization and an information object.</rdfs:comment>
        <rdfs:label xml:lang="en">realizes</rdfs:label>
    </ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/instrument/hasBuilder -->

    <ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/instrument/hasBuilder">
