                <allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/TextFragment"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/isMusicEntityPartOf"/>
