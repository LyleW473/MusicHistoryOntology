    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith -->

    <ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/MON/person.owl#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/MON/person.owl#Person"/>
        <rdfs:comment>A catch-all object property, useful for alignment and querying purposes.
It is declared as both transitive and symmetric, in order to reason an a maximal closure of associations between individuals.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"/>
        <rdfs:label>associatedWith</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasComposer -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasComposer">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasMusician"/>
        <inverseOf rdf:resource="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#isComposerOF"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Musician"/>
        <rdfs:comment xml:lang="en">Relates a musical work to the person who composed its music.</rdfs:comment>
        <rdfs:label xml:lang="en">has composer</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasConductor -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasConductor">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasMusician"/>
        <inverseOf rdf:resource="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#isConductorOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Musician"/>
        <rdfs:comment xml:lang="en">Relates a person to a musical organization (e.g., orchestra) in the role of conductor.</rdfs:comment>
        <rdfs:label xml:lang="en">has conductor</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasEventInterval -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasEventInterval">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasTimeInterval"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#PerformanceEvent"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
        <rdfs:comment>Specifies the time interval a performance event lasted.</rdfs:comment>
        <rdfs:label>has time interval for a PerformanceEvent.</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasLyricist -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/lianmatsuo/ontologies/2025/2/history_of_music#hasLyricist">
