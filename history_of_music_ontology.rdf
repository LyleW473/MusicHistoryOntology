                <onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/hasStartTime"/>
                <maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</maxQualifiedCardinality>
                <onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A time interval.</rdfs:comment>
        <rdfs:label xml:lang="en">Time interval</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/core/Type -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/core/Type">
        <rdfs:comment xml:lang="en">The concept of a type.</rdfs:comment>
        <rdfs:label xml:lang="en">Type</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/instrument/Instrument -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/instrument/Instrument">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/InformationObject"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/instrument/wasInventedAtTime"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/instrument/wasInventedBy"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/core/Agent"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An instrument intended as an information object (e.g. the guitar as an information object), regardless its physical realizations (e.g. the specific guitar at my place).</rdfs:comment>
        <rdfs:label xml:lang="en">Instrument</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/instrument/InstrumentRealization -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/instrument/InstrumentRealization">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/InformationRealization"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/realizes"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/instrument/Instrument"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/instrument/wasBuiltIn"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/core/Place"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The physical realization of an instrument, intended as the information realization (e.g. the specific guitar at my place) and the information object (the guitar as an information object).</rdfs:comment>
        <rdfs:label xml:lang="en">Instrument realization</rdfs:label>
    </Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-meta/AbstractScore -->

    <Class rdf:about="https://w3id.org/polifonia/ontology/music-meta/AbstractScore">
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasCompositionPart"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/CompositionPart"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasFormType"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/FormType"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasInstrumentation"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Instrumentation"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasKey"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Key"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasText"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Text"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/isMusicEntityPartOf"/>
                <someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MusicEntity"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasCompositionPart"/>
                <allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/CompositionPart"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasFormType"/>
                <allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/FormType"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasInstrumentation"/>
                <allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Instrumentation"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasKey"/>
                <allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-meta/Key"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-meta/hasText"/>
